    public List<Book> getAllBooks() {
        book.add(new Book("ダイヤのA1巻","寺嶋裕二","講談社",LocalDate.of(2006,9,15),10));
        book.add(new Book("ダイヤのA2巻","寺嶋裕二","講談社",LocalDate.of(2006,9,15),10));
        book.add(new Book("ダイヤのA3巻","寺嶋裕二","講談社",LocalDate.of(2006,9,15),10));
        book.add(new Book("ダイヤのA4巻","寺嶋裕二","講談社",LocalDate.of(2006,9,15),10));
        book.add(new Book("ダイヤのA5巻","寺嶋裕二","講談社",LocalDate.of(2006,9,15),10));
    }
}
